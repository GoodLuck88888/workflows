<odoo>
  <data>

    <record model="ir.ui.view" id="xw_auth_demo.list">
      <field name="name">xw_auth_demo list</field>
      <field name="model">xw_auth_demo.xw_auth_demo</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          <field name="value"/>
          <field name="value2"/>
          <button name="test_raise" type="object" string="Action"/>
        </tree>
      </field>
    </record>


    <!-- actions opening views on models -->
    <record model="ir.actions.act_window" id="xw_auth_demo.action_window">
      <field name="name">xw_auth_demo window</field>
      <field name="res_model">xw_auth_demo.xw_auth_demo</field>
      <field name="view_mode">tree,form</field>
    </record>


    <!-- server action to the one above -->

    <record model="ir.actions.server" id="xw_auth_demo.action_server">
      <field name="name">xw_auth_demo server</field>
      <field name="model_id" ref="model_xw_auth_demo_xw_auth_demo"/>
      <field name="state">code</field>
      <field name="code">
        action = {
          "type": "ir.actions.act_window",
          "view_mode": "tree,form",
          "res_model": model._name,
        }
      </field>
    </record>


    <!-- Top menu item -->
    <menuitem name="xw_auth_demo" id="xw_auth_demo.menu_root"/>

    <!-- menu categories -->
    <menuitem name="Menu 1" id="xw_auth_demo.menu_1" parent="xw_auth_demo.menu_root"/>
    <menuitem name="Menu 2" id="xw_auth_demo.menu_2" parent="xw_auth_demo.menu_root"/>

    <!-- actions -->
    <menuitem name="List" id="xw_auth_demo.menu_1_list" parent="xw_auth_demo.menu_1"
              action="xw_auth_demo.action_window"/>

    <menuitem name="Server to list" id="xw_auth_demo" parent="xw_auth_demo.menu_2"
              action="xw_auth_demo.action_server"/>

  </data>
</odoo>